= stylesheet_link_tag 'popups/alert-popup'

.alert-popup
  %h1 Upgrade
  .text
    %br
    This feature is not available in this package. 
    %br
    Would you like to upgrade?
  .popup-action-bar
    = link_to_function "No", "hidePopup();", :class => 'cancel'
    = link_to_remote "Yes", :url => preferences_path, :method => :get
